        <Viewer:CrystalReportsViewer Name="reportViewer" Margin="0,32,-0.4,0.4"></Viewer:CrystalReportsViewer>
        <Button Content="Back" HorizontalAlignment="Left" Margin="1199,0,-0.4,681.6" Width="75" Background="Red" Foreground="White" BorderBrush="Red" Height="Auto" Click="Button_Click"/>
    </Grid>
</Window>
