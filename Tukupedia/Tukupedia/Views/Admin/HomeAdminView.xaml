            <TextBox Height="41" Canvas.Left="776" TextWrapping="Wrap" Canvas.Top="285" Width="300" FontSize="20"/>
            <Label Content="No Telp" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="18" Canvas.Left="776" Canvas.Top="331"/>
            <TextBox Height="41" Canvas.Left="776" TextWrapping="Wrap" Canvas.Top="368" Width="300" FontSize="20"/>
