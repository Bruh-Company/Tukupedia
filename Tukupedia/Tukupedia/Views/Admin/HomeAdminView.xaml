            <Button Content="Banned User" Canvas.Left="933" Canvas.Top="540" Width="143"/>
            <Label Content="Alamat" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="18" Canvas.Left="776" Canvas.Top="170"/>
